{
  "name": "regex-repo",
  "version": "1.1.2",
  "description": "Collection of JS regular expressions.",
  "main": "dist/regex-repo.js",
  "files": [
    "dist/",
    "LICENSE.txt",
    "README.md"
  ],
  "scripts": {

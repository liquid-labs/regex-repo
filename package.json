  },
  "license": "Apache-2.0",
  "keywords": [
    "css",
    "data-validation",
    "javascript",
    "regex",
    "regexp",
    "regular expression",
    "validate",
    "validate-data",
    "validation"
  ],
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "bugs": {
    "url": "https://github.com/liquid-labs/regex-repo/issues"
  },
  "homepage": "https://github.com/liquid-labs/regex-repo#readme",
  "devDependencies": {
    "@liquid-labs/catalyst-resource-babel-and-rollup": "^1.0.0-alpha.3",
    "@liquid-labs/catalyst-resource-eslint": "^1.0.0-alpha.1",
    "@liquid-labs/catalyst-resource-jest": "^1.0.0-alpha.3"
  },
  "catalyst": {
    "jestConfig": {
      "collectCoverageFrom": [
        "**/*.{js,cjs,mjs,jsx}",
